<Loggers>
    <!-- Disable Apache HTTP Client Logs -->
    <Logger name="org.apache.hc" level="WARN" additivity="false">
        <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="org.apache.hc.client5.http.wire" level="WARN" additivity="false">
        <AppenderRef ref="Console"/>
    </Logger>
    <Root level="INFO">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
